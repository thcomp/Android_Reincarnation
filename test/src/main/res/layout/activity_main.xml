<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:id="@+id/activity_main"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="jp.co.thcomp.test.MainActivity"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnStartTestA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start testA" />

    <Button
        android:id="@+id/btnStartTestA_All"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start testA All" />

    <Button
        android:id="@+id/btnStartTestB"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start testB" />

    <Button
        android:id="@+id/btnStartTestB_All"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start testB All" />

    <Button
        android:id="@+id/btnStartTestC"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start testC" />

    <Button
        android:id="@+id/btnStartTestUri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="start test uri" />

</LinearLayout>
